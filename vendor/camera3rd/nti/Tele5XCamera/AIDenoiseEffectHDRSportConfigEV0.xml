        <ExpTimeStart>0</ExpTimeStart>
        <ExpTimeEnd>10000</ExpTimeEnd>
        <EvScale>1</EvScale>
        <EvBias>0</EvBias>
        <ResScale>0.925</ResScale>
    </DenoiserBranch>
	<DenoiserBranch>
        <ISOstart>5800</ISOstart>
        <ISOend>10000</ISOend>
        <ExpTimeStart>0</ExpTimeStart>
        <ExpTimeEnd>10000</ExpTimeEnd>
        <EvScale>1</EvScale>
