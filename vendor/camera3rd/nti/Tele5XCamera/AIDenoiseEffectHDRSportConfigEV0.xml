    </DenoiserBranch>
    <!-- HISO模式下的降噪参数 -->
    <DenoiserBranch>
        <ISOstart>10000</ISOstart>
        <ISOend>65000</ISOend>
        <ExpTimeStart>0</ExpTimeStart>
        <ExpTimeEnd>10000</ExpTimeEnd>
        <EvScale>1</EvScale>
        <EvBias>0</EvBias>
        <ResScale>1.0</ResScale>
    </DenoiserBranch>
</AiDenoiseConfig>