    <!-- Model selection based on ISO. ISO min-low: w/o aisr, ISO low-middle: 1xlowiso, ISO middle-high: 1x/2x/4x, ISO high-max: 1xhighiso. -->
    <ISOParam>
        <x1>
            <low>0</low>
            <middle>1600</middle>
            <high>99999</high>
        </x1>

        <x2>
            <low>0</low>
            <middle>99999</middle>
            <high>99999</high>
        </x2>

        <x5>
            <low>0</low>
            <middle>0</middle>
            <high>1600</high>
        </x5>
    </ISOParam>

    <!-- Model selection based on digitalZoom. -->
    <ZoomParam>
        <x1>
            <sisr2x>100</sisr2x>
            <sisr4x>100</sisr4x>
        </x1>

        <x2>
            <sisr2x>100</sisr2x>
            <sisr4x>100</sisr4x>
        </x2>

        <x5>
            <sisr2x>100</sisr2x>
            <sisr4x>100</sisr4x>
        </x5>
    </ZoomParam>

    <!-- final sharp parameter = x1SharpParam/x2SharpParam/x5SharpParam * SharpStrength -->
    <SharpFunc use="1">
        <SharpStrength>1</SharpStrength>
        <SharpenThreshold>0</SharpenThreshold>
        <!-- Sharp patameters. 
        DigitalZoom min-zoomMin: (ISO min-low: low, ISO low-max: 0), 
        DigitalZoom zoomMin-max: (ISO min-low: low, ISO low-high: middle, ISO high-max: high). -->
        <x1>
            <low>0</low>
            <middle>0</middle>
            <high>0</high>
            <zoomMin>1</zoomMin>
        </x1>

        <x2>
            <low>0</low>
            <middle>0</middle>
            <high>0</high>
            <zoomMin>1</zoomMin>
        </x2>

        <x5>
            <low>0</low>
            <middle>0</middle>
            <high>0</high>
            <zoomMin>1</zoomMin>
        </x5>
    </SharpFunc>


    <!-- Noise-Leak patameters(0-1). The higher value, the more noise will leak. 
    DigitalZoom min-zoomMax: (ISO low-middle: low, ISO middle-high: middle, ISO high-max: high), 
    DigitalZoom zoomMax-max: 1. -->
    <NoiseLeakParam>
        <x1NoiseLeakParam>
            <low>1</low>
            <middle>1</middle>
            <high>1</high>
            <zoomMax>100</zoomMax>
        </x1NoiseLeakParam>

        <x2NoiseLeakParam>
            <low>0.8</low>
            <middle>1</middle>
            <high>1</high>
            <zoomMax>100</zoomMax>
        </x2NoiseLeakParam>

        <x5NoiseLeakParam>
            <low>1</low>
            <middle>1</middle>
            <high>1</high>
            <zoomMax>100</zoomMax>
        </x5NoiseLeakParam>
    </NoiseLeakParam>
</AIEnhaceConfig>