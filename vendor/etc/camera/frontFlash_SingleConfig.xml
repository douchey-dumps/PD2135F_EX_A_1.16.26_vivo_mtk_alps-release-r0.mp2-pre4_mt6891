<?xml version="1.0" encoding="UTF-8"?>
 <!--说明-->
 <!--请将本配置文件放置于手机内部存储空间根目录下-->
 <!--请不要更改本配置文件的文件名-->
 <!--修改参数的时候请直接修改对应项的数字数值，请不要更改其他内容-->
 <!--除了亮度阈值为int型，其他参数均为double型-->
 <!--若除亮度阈值外的其他参数刚好需要修改为整数，如3，需要写成3.0，亮度阈值可以直接写成3-->
 
 
 <documents>
	 <!--容许红色色偏的阈值，默认为 2.0-->
	<redTh>2.0</redTh>
	 <!--容许蓝色色偏的阈值，默认为 3.0-->
	<blueTh>3.0</blueTh>
	 <!--容许绿色色偏的阈值，默认为 3.0-->
	<greenTh>3.0</greenTh>
	 <!--容许黄色色偏的阈值，默认为 3.5-->
	<yellowTh>3.5</yellowTh>
	 <!--暗角参数,数值越小范围越大，在0.45~0.65之间，默认值设置为0.45-->
	<shading>0.45</shading>
	 <!--色偏参数,数值越大范围越大，在0.05~0.08之间，默认值设置为0.07-->
	<colorDiff>0.07</colorDiff>
	 <!--亮度的阈值，默认180-->
	<brightTh>180</brightTh>
</documents>
